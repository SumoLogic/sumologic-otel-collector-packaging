receivers:
  filelog/haproxy/localhost:
    include_file_name: false
    include_file_path: true
    operators:
      - type: move
        from: attributes["log.file.path"]
        to: resource["log.file.path"]
    include:
      - /var/log/haproxy*.log
processors:
  resource/haproxy_resource_attributes/localhost:
    attributes:
      - key: sumo.datasource
        value: haproxy
        action: insert
      - key: webengine.system
        value: haproxy
        action: insert
      - key: proxy.cluster
        value: test
        action: insert
service:
  pipelines:
    logs/haproxy/localhost:
      receivers:
        - filelog/haproxy/localhost
      processors:
        - memory_limiter
        - resourcedetection/system
        - resource/haproxy_resource_attributes/localhost
        - batch
      exporters:
        - sumologic
