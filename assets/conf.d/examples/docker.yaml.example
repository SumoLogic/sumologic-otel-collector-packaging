receivers:
  filelog/docker/events/localhost:
    include_file_name: false
    include:
      - /var/docker/events/conainer.json
    operators:
      - type: json_parser
  docker_stats/localhost:
    excluded_images:
      - dummyContainer1
      - dummyContainer2
    metrics:
      container.cpu.usage.system:
        enabled: true
      container.cpu.throttling_data.periods:
        enabled: true
      container.cpu.throttling_data.throttled_time:
        enabled: true
      container.memory.usage.max:
        enabled: true
      container.memory.pgfault:
        enabled: true
      container.memory.rss:
        enabled: true
      container.memory.unevictable:
        enabled: true
      container.network.io.usage.rx_errors:
        enabled: true
      container.network.io.usage.rx_packets:
        enabled: true
      container.network.io.usage.tx_errors:
        enabled: true
      container.network.io.usage.tx_packets:
        enabled: true
      container.blockio.io_serviced_recursive:
        enabled: true
processors:
  resource/docker_resource_attributes/localhost:
    attributes:
      - key: sumo.datasource
        value: docker
        action: insert
      - key: _sourceCategory
        value: otel/docker
        action: insert
service:
  pipelines:
    metrics/docker/localhost:
      receivers:
        - docker_stats/localhost
      processors:
        - memory_limiter
        - resourcedetection/system
        - resource/docker_resource_attributes/localhost
        - batch
      exporters:
        - sumologic
    logs/docker/localhost:
      receivers:
        - filelog/docker/events/localhost
      processors:
        - memory_limiter
        - resourcedetection/system
        - resource/docker_resource_attributes/localhost
        - batch
      exporters:
        - sumologic
